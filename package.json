{
  "name": "spur-monocle-manager",
  "version": "1.0.1",
  "description": "Wrapper for the Monocle SDK (Spur)",
  "keywords": [
    "monocle",
    "spur",
    "sdk",
    "loader"
  ],
  "homepage": "https://github.com/Xavier4492/spur-monocle-manager#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Xavier4492/spur-monocle-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Xavier4492/spur-monocle-manager/issues"
  },
  "author": "Xavier4492 <xavierloue@gmail.com>",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "prepare": "npm run build",
    "lint": "eslint \"src/**/*.{ts,js}\" \"tests/**/*.{ts,js}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest --run --coverage",
    "docs:api": "typedoc",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.2",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/estree": "^1.0.7",
    "@types/json-schema": "^7.0.15",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-istanbul": "^3.1.4",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "semantic-release": "^24.2.4",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=6"
  },
  "publishConfig": {
    "access": "public"
  }
}
